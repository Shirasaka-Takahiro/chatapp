<div class="container">
 <h3>Chatroom一覧</h3>
  <div>
    <ul>
      <% @rooms.each do |room| %>
        <li><%= link_to "ROOM#{room.id}", room_path(room) %></li>
      <% end %>
    </ul>
  </div>
  <% if current_user.try(:admin?) %>
    <%= link_to "管理者画面", admin_users_path, class: 'btn' %>
  <% end %>
</div>